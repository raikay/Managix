{
  "testName": "appsetting",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  //日志
  "log": {
    //操作日志
    "operation": true
  },
  //跨域地址
  "corUrls": [ "http://*:9000" ],
  //验证码
  "varifyCode": {
    //启用
    "enable": true,
    //字体列表
    "fonts": [ "Times New Roman", "Verdana", "Arial", "Gungsuh", "Impact" ]
  },
  //统一认证授权服务器
  "IdentityServer": {
    //启用
    "enable": false,
    //地址，开发认证地址前往appconfig.Development.json修改
    "url": "http://127.0.0.1:8081"
  }
}
